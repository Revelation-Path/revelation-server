  "query": "\n        UPDATE users SET\n            name = COALESCE($2, name),\n            gender = COALESCE($3, gender),\n            birth_date = COALESCE($4, birth_date),\n            confession_id = COALESCE($5, confession_id),\n            updated_at = NOW()\n        WHERE id = $1\n        RETURNING\n            id,\n            name,\n            gender as \"gender: _\",\n            birth_date,\n            confession_id,\n            email,\n            phone,\n            telegram_id,\n            created_at,\n            updated_at\n        ",
