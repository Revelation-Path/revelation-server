  "query": "\n            SELECT\n                vr.id, vr.user_id,\n                u.name as user_name,\n                vr.content, vr.created_at\n            FROM verse_responses vr\n            JOIN users u ON u.id = vr.user_id\n            WHERE vr.daily_reading_id = $1\n            ORDER BY vr.created_at ASC\n            ",
